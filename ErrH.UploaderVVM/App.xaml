<Application x:Class="ErrH.UploaderVVM.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ErrH.UploaderVVM"
             xmlns:vw="clr-namespace:ErrH.UploaderVVM.Views"
             xmlns:vm="clr-namespace:ErrH.UploaderVVM.ViewModels">
    <Application.Resources>
        <ResourceDictionary>
                        
            <DataTemplate DataType="{x:Type vm:FilesListViewModel}">
                <vw:FilesGridCtrl />
            </DataTemplate>

            <DataTemplate DataType="{x:Type vm:AllAppFoldersVM}">
                <vw:AppsListCtrl />
            </DataTemplate>


        </ResourceDictionary>
    </Application.Resources>
</Application>
